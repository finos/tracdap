  "files": [
    "trac.*"
  ],
  "main": "trac.js",
  "dependencies": {
    "google-protobuf": "^3.15.7",
    "grpc-web": "^1.2.1",
    "protobufjs": "~6.10.1"
  },
  "devDependencies": {
    "adm-zip": "^0.5.5",
    "follow-redirects": "^1.13.3",
